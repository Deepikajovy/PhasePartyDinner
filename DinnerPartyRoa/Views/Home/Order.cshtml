@using DinnerPartyRoa.Models
@model List<Order>

<div id="OrderTable">

    <table id="Orders">
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.Item.Title</td>
                <td>@order.User.Name</td>
                <td>@order.CreatedOn</td>
            </tr>
        }
    </table>
</div>